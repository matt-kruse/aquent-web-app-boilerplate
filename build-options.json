{
	"verbose": true,
	"concat_style": true,
	"config_variable_open":"#{#",
	"config_variable_close":"#}#",
	"combined_css_file":"style.css",
	"AUTOPREFIXER_BROWSERS": [
		"ie >= 10",
		"ie_mob >= 10",
		"ff >= 30",
		"chrome >= 34",
		"safari >= 7",
		"opera >= 23",
		"ios >= 7",
		"android >= 4.4",
		"bb >= 10"
	],
	"paths": {
	  "less": "app/**/*.less",
	  "css": "app/**/*.css",
	  "js": "app/**/*.js",
	  "html": "app/**/*.html",
	  "image": "app/images/**/*",
	  "all": "app/**/*",
	  "fonts": "app/fonts/**/*",
	  "dist": "./dist/",
	  
	  "ignore": "**/_*"
	}	
}
